<script>
    $(async function(){

        async function getDashboardTopInformation(){

            let response = await fetch("/provider/dashboard/top-information", {
                headers: { 'Content-Type': 'application/json' },
                method: "POST",
                body: JSON.stringify({}),
            });

           let topDashboardInformation = await response.json();

           console.log(topDashboardInformation)

            $("#totalRevenue").html(topDashboardInformation.totalRevenue);
            $("#totalServicemen").html(topDashboardInformation.totalServicemen);
            $("#totalServices").html(topDashboardInformation.totalServices);
            $("#totalBookings").html(topDashboardInformation.totalBookings);
            $("#totalBalance").html(topDashboardInformation.totalBalance);
            $("#totalAddedBookings").html(topDashboardInformation.bookingStatusesCount.added);
            $("#totalOngoingBookings").html(topDashboardInformation.bookingStatusesCount.ongoing);
            $("#totalOnthewayBookings").html(topDashboardInformation.bookingStatusesCount.ontheway);
            $("#totalCompletedBookings").html(topDashboardInformation.bookingStatusesCount.completed);
            $("#totalOnholdBookings").html(topDashboardInformation.bookingStatusesCount.hold);
            $("#totalCancelledBookings").html(topDashboardInformation.bookingStatusesCount.cancelled);
        }

        await getDashboardTopInformation();
    })
   
</script>